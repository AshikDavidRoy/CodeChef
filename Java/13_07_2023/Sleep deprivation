import java.util.Scanner;

public class SleepDeprivation {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);

        int T = scanner.nextInt(); // Number of test cases

        for (int i = 0; i < T; i++) {
            int X = scanner.nextInt(); // Number of hours Chef slept

            if (X < 7) {
                System.out.println("YES"); // Chef is sleep-deprived
            } else {
                System.out.println("NO"); // Chef is not sleep-deprived
            }
        }

        scanner.close();
    }
}
