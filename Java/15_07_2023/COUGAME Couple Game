import java.util.Scanner;

class Main {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        int T = scanner.nextInt(); // number of test cases

        for (int i = 0; i < T; i++) {
            int G = scanner.nextInt(); // number of girl students
            int B = scanner.nextInt(); // number of boy students

            int minBoysNotParticipating = B - G ;
            System.out.println(minBoysNotParticipating);
        }

        scanner.close();
    }
}
