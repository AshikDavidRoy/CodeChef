import java.util.Scanner;

class Main {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        int numTestCases = scanner.nextInt(); // number of test cases

        for (int i = 0; i < numTestCases; i++) {
            int numCourses = scanner.nextInt(); // number of courses
            int numUnitsPerCourse = scanner.nextInt(); // number of units per course
            int numChaptersPerUnit = scanner.nextInt(); // number of chapters per unit

            int totalChapters = numCourses * numUnitsPerCourse * numChaptersPerUnit;
            System.out.println(totalChapters);
        }

        scanner.close();
    }
}
