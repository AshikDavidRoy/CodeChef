import java.util.Scanner;

public class ContestPrizeMoney {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        int T = scanner.nextInt();  // Number of test cases
        
        for (int i = 0; i < T; i++) {
            int X = scanner.nextInt();  // Prize for top 10 rankers
            int Y = scanner.nextInt();  // Prize for ranks 11 to 100
            
            int totalPrizeMoney = X * 10 + Y * 90;
            System.out.println(totalPrizeMoney);
        }
        
        scanner.close();
    }
}
