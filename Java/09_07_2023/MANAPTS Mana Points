import java.util.Scanner;

public class SpecialAttacks {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        int T = scanner.nextInt();  // Number of test cases
        
        for (int i = 0; i < T; i++) {
            int X = scanner.nextInt();  // Cost of one special attack
            int Y = scanner.nextInt();  // Number of mana points
            
            int maxAttacks = Y / X;  // Maximum number of special attacks
            
            System.out.println(maxAttacks);
        }
        
        scanner.close();
    }
}
