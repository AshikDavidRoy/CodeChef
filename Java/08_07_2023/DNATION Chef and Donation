import java.util.Scanner;

public class IncomeDonation {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        int T = scanner.nextInt();  // Number of test cases
        
        for (int i = 0; i < T; i++) {
            int X = scanner.nextInt();  // Income of Chef
            int Y = scanner.nextInt();  // Income of Chefina
            
            int donationAmount = Y - X;  // Calculate the amount to be donated
            
            System.out.println(donationAmount);
        }
        
        scanner.close();
    }
}
