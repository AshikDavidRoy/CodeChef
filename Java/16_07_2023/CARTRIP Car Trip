import java.util.Scanner;

public class CarRent {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int t = sc.nextInt();

        for (int i = 0; i < t; i++) {
            int x = sc.nextInt();
            int cost = (x >= 300) ? (x * 10) : 3000;

            System.out.println(cost);
        }

        sc.close();
    }
}
