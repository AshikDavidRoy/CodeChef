# Step 1
X, Y = map(float, input().split())

# Step 2
if X % 5 == 0 and Y >= X + 0.50:
    # Step 2.1
    Y -= X + 0.50

# Step 3
print("{:.2f}".format(Y))
##############################################---------OR---------########################################


n,atm=map(float,input().split())
n=int(n)
if (n+0.5<=atm and n%5==0):
    print(float(atm-n-0.5))
else:
    print(float(atm))
